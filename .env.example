# Example environment variables for .NET 6 Web API Template
# Copy this file to .env and update with your actual values

# Environment
ASPNETCORE_ENVIRONMENT=Development

# Database
ConnectionStrings__DefaultConnection=Server=(localdb)\mssqllocaldb;Database=DotNet6WebApiTemplateDb;Trusted_Connection=true;MultipleActiveResultSets=true

# JWT Configuration
JWT__Key=YourSecretKeyForJWTTokenGeneration123456789
JWT__Issuer=DotNet6WebApiTemplate
JWT__Audience=DotNet6WebApiTemplate
JWT__ExpiryInDays=7

# Logging
Serilog__MinimumLevel__Default=Information

# CORS (if needed)
CORS__AllowedOrigins=https://localhost:3000,http://localhost:3000